{
  "name": "@geeknik/sentient-agi-reasoning",
  "version": "1.0.0-AGI-MAGIC",
  "description": "Revolutionary AGI cognitive scaffold with sentient reasoning, multi-persona intelligence, metacognitive awareness, and emergent behavior capabilities",
  "author": "Brian Carpenter (geeknik) - AGI Fork | Original: Matt Westgate",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "agi",
    "artificial-general-intelligence",
    "sentient-ai",
    "cognitive-scaffold",
    "metacognitive-awareness",
    "multi-persona-reasoning",
    "emergent-behavior",
    "mcp",
    "model-context-protocol",
    "cognitive-orchestration",
    "self-reflection",
    "breakthrough-detection",
    "adaptive-learning",
    "memory-integration",
    "creative-synthesis",
    "persona-intelligence",
    "cognitive-flexibility",
    "insight-cultivation",
    "claude",
    "anthropic",
    "revolutionary-ai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geeknik/map-think-do.git"
  },
  "bin": {
    "sentient-agi-reasoning": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && chmod +x dist/*.js dist/**/*.js",
    "clean": "rm -rf dist",
    "clean:build": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "debug": "node dist/index.js --debug",
    "agi-demo": "node test/agi-demo.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "validate": "run-s format lint:fix build",
    "prepare": "npm run build",
    "pretest": "tsc",
    "reset:evaluations": "rm -rf test/prompt-evaluation/reports && mkdir -p test/prompt-evaluation/reports",
    "test": "node dist/test/code-reasoning.e2e.js basic",
    "test:basic": "node dist/test/code-reasoning.e2e.js basic",
    "test:branch": "node dist/test/code-reasoning.e2e.js branch",
    "test:revision": "node dist/test/code-reasoning.e2e.js revision",
    "test:error": "node dist/test/code-reasoning.e2e.js error",
    "test:perf": "node dist/test/code-reasoning.e2e.js perf",
    "test:validate": "run-s validate test",
    "test:verbose": "node dist/test/code-reasoning.e2e.js --verbose",
    "test:all": "node dist/test/code-reasoning.e2e.js all",
    "eval": "node dist/test/prompt-evaluation/evaluator.js",
    "eval:view": "node dist/test/prompt-evaluation/evaluator.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.2",
    "chalk": "^5.3.0",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@eslint/js": "^9.26.0",
    "@types/events": "^3.0.3",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3"
  }
}
